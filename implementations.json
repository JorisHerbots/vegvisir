{
	"aioquic": {
		"image": "aiortc/aioquic-qns",
		"url": "https://github.com/aiortc/aioquic",
		"role": [
			"server"
		],
		"client": {
			"type": "docker"
		}
	},
	"godash": {
		"image": "vegvisir-containers/godash:latest",
		"url": "https://github.com/uccmisl/godash",
		"role": [
			"client"
		],
		"client": {
			"type": "docker"
		},
		"env": [
			{"name": "ABR", "description": "-adapt parameter", "default": "arbiter"},
			{"name": "CODEC", "description": "-codec parameter", "default": "h264"},
			{"name": "INIT_BUFFER", "description": "-initBuffer parameter", "default": "2"},
			{"name": "MAX_BUFFER", "description": "-maxBuffer parameter", "default": "20"},
			{"name": "MAX_HEIGHT", "description": "-maxHeight parameter", "default": "1080"},
			{"name": "STREAM_DURATION", "description": "-streamDuration parameter", "default": "0.2"},
			{"name": "EXP_RATIO", "description": "-expRatio parameter", "default": "0.2"},
			{"name": "STREAM_SPEED", "description": "-streamSpeed parameter", "default": "1"}
		]
	},
	"qns": {
		"image": "martenseemann/quic-network-simulator",
		"url": "https://github.com/marten-seemann/quic-network-simulator",
		"role": [
			"shaper"
		],
		"env": [
			{"name": "argument", "description": "argument", "default": "simple-p2p --delay=15ms --bandwidth=10Mbps --queue=25", "type": "dropdown", 
			"options": ["simple-p2p --delay=15ms --bandwidth=10Mbps --queue=25"]
			}
		]
	},
	"tc-netem": {
		"image": "vegvisir-containers/tc-netem",
		"url": "tc-netem",
		"role": [
			"shaper"
		],
		"active": true,
		"env": [
			{"name": "argument", "description": "argument", "default": "simple 15 10", "type": "dropdown", 
			"options": ["simple 15 10", "akamai_cellular_emulation.sh loss_based median 37", "akamai_cellular_emulation.sh experience_based fair 37"]
			}
			]
	},
	"chrome": {
		"name": "Google Chrome unstable",
		"description": "google-chrome-unstable",
		"url": "https://www.google.com/chrome/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "google-chrome-unstable --origin-to-force-quic-on={origin} --enable-experimental-web-platform-features --log-net-log={client_log_dir}/net-log.json --autoplay-policy=no-user-gesture-required --auto-open-devtools-for-tabs --ignore-certificate-errors-spki-list={cert_fingerprint} https://{origin}/dashjs-qlog-abr/demo/demo.html?autosave=true&video={request_urls}"
		}
	},
	"firefox-nightly": {
		"url": "https://www.mozilla.org/en-US/firefox/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "firefox-nightly {request_urls}"
		}
	},
	"chrome_unstable_94.0.4595.0": {
		"name": "Google Chrome unstable",
		"description": "chrome-unstable-94.0.4595.0",
		"url": "https://www.google.com/chrome/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "./applications/chrome-unstable-94.0.4595.0/chrome --origin-to-force-quic-on={origin} --enable-experimental-web-platform-features --log-net-log={client_log_dir}/net-log.json --autoplay-policy=no-user-gesture-required --auto-open-devtools-for-tabs --ignore-certificate-errors-spki-list={cert_fingerprint} https://{origin}/dashjs-qlog-abr/demo/demo.html?autosave=true&video={request_urls}",
			"setup": [
				{
					"sudo": true,
					"replace_tilde": true,
					"command": "rm -r ~/.cache/google-chrome-unstable/"
				},
				{
					"sudo": true,
					"replace_tilde": true,
					"command": "rm -r ~/.cache/google-chrome/"
				},
				{
					"sudo": false,
					"replace_tilde": true,
					"command": "python ./util/chrome-set-downloads-folder.py ~/.config/google-chrome/Default/Preferences \"{client_log_dir}\""
				},
				{
					"sudo": false,
					"replace_tilde": true,
					"command": "python ./util/chrome-set-downloads-folder.py ~/.config/google-chrome-unstable/Default/Preferences \"{client_log_dir}\""
				}
			]
		}
	},
