{
	"aioquic": {
		"image": "aiortc/aioquic-qns",
		"url": "https://github.com/aiortc/aioquic",
		"role": [
			"server",
			"client"
		],
		"client": {
			"type": "docker"
		}
	},
	"godash": {
		"image": "vegvisir-containers/godash:latest",
		"url": "https://github.com/uccmisl/godash",
		"role": [
			"client"
		],
		"client": {
			"type": "docker"
		},
		"env": [
			"ABR",
			"CODEC",
			"INIT_BUFFER",
			"MAX_BUFFER",
			"MAX_HEIGHT",
			"STREAM_DURATION",
			"EXP_RATIO",
			"STREAM_SPEED"
		]
	},
	"qns": {
		"image": "martenseemann/quic-network-simulator",
		"url": "https://github.com/marten-seemann/quic-network-simulator",
		"role": [
			"shaper"
		],
		"active": true,
		"scenarios": {
			"simple-p2p": {
				"arguments": "\"simple-p2p --delay=15ms --bandwidth=10Mbps --queue=25\"",
				"active": true
			}
		}
	},
	"tc-netem": {
		"image": "vegvisir-containers/tc-netem",
		"url": "",
		"role": [
			"shaper"
		],
		"active": true,
		"scenarios": {
			"simple": {
				"arguments": "\"simple\"",
				"active": true
			},
			"akamai cellular loss": {
				"arguments": "\"akamai_cellular_emulation.sh loss_based median 37\"",
				"active": false
			},
			"akamai cellular experience": {
				"arguments": "\"akamai_cellular_emulation.sh experience_based fair 37\"",
				"active": false
			}
		}
	},
	"chrome": {
		"url": "https://www.google.com/chrome/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "google-chrome-unstable --origin-to-force-quic-on={origin} --enable-experimental-web-platform-features --autoplay-policy=no-user-gesture-required --auto-open-devtools-for-tabs --ignore-certificate-errors-spki-list={cert_fingerprint} https://{origin}/dashjs-qlog-abr/demo/demo.html?autosave=true&video={request_urls}"
		}
	},
	"firefox-nightly": {
		"url": "https://www.mozilla.org/en-US/firefox/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "firefox-nightly {request_urls}"
		}
	},
	"chrome_unstable_94.0.4595.0": {
		"url": "https://www.google.com/chrome/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "./applications/chrome-unstable-94.0.4595.0/chrome --origin-to-force-quic-on={origin} --enable-experimental-web-platform-features --autoplay-policy=no-user-gesture-required --auto-open-devtools-for-tabs --ignore-certificate-errors-spki-list={cert_fingerprint} https://{origin}/dashjs-qlog-abr/demo/demo.html?autosave=true&video={request_urls}",
			"setup": [
				{
					"sudo": true,
					"replace_tilde": true,
					"command": "rm -r ~/.cache/google-chrome-unstable/"
				},
				{
					"sudo": true,
					"replace_tilde": true,
					"command": "rm -r ~/.cache/google-chrome/"
				},
				{
					"sudo": false,
					"replace_tilde": true,
					"command": "python ./util/chrome-set-downloads-folder.py ~/.config/google-chrome/Default/Preferences \"{client_log_dir}\""
				},
				{
					"sudo": false,
					"replace_tilde": true,
					"command": "python ./util/chrome-set-downloads-folder.py ~/.config/google-chrome-unstable/Default/Preferences \"{client_log_dir}\""
				}
			]
		}
	},
